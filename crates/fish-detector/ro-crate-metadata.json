    {
      "@id": "#acceptance-test-async",
      "@type": "HowTo",
      "name": "Asynchronous Fish-Detector acceptance test",
      "description": "Upload a sample image, wait for detection to complete remotely, then download the prediction artifacts and check the expected confirmation text.",
      "tool": [
        {
          "@id": "#tool-oscar-cli"
        }
      ],
      "supply": [
        {
          "@id": "#supply-sample-image"
        }
      ],
      "step": [
        {
          "@id": "#step-async-put"
        },
        {
          "@id": "#step-async-wait"
        },
        {
          "@id": "#step-async-get"
        }
      ]
    },
    {
      "@id": "#tool-oscar-cli",
      "@type": "HowToTool",
      "name": "OSCAR CLI",
      "item": {
        "@id": "#oscar-cli"
      }
    },
    {
      "@id": "#supply-sample-image",
      "@type": "HowToSupply",
      "name": "Sample detection image",
      "item": {
        "@id": "input.jpg"
      }
    },
    {
      "@id": "#expected-image",
      "@type": [
        "File",
        "ImageObject"
      ],
      "name": "Expected detection output",
      "description": "JPEG image produced by the service containing annotated detections and JSON file containing the detection results.",
      "encodingFormat": "image/jpeg"
    },
    {
      "@id": "#step-async-put",
      "@type": "HowToStep",
      "position": 1,
      "text": "Upload the sample detection image to the service storage.",
      "potentialAction": {
        "@id": "#action-async-put"
      }
    },
    {
      "@id": "#step-async-wait",
      "@type": "HowToStep",
      "position": 2,
      "text": "Wait for the remote inference to produce detection outputs.",
      "timeRequired": "PT300S"
    },
    {
      "@id": "#step-async-get",
      "@type": "HowToStep",
      "position": 3,
      "text": "Download the latest output artifacts and confirm that it is the expected detection JPG and JSON.",
      "potentialAction": {
        "@id": "#action-async-get"
      }
    },
    {
      "@id": "#action-async-put",
      "@type": "TransferAction",
      "name": "service put-file",
      "object": {
        "@id": "input.jpg"
      },
      "target": {
        "@id": "#entry-async-put"
      },
      "additionalProperty": [
        {
          "@id": "#command-template-async-put"
        }
      ]
    },
    {
      "@id": "#entry-async-put",
      "@type": "EntryPoint",
      "actionApplication": {
        "@id": "#oscar-cli"
      }
    },
    {
      "@id": "#command-template-async-put",
      "@type": "PropertyValue",
      "propertyID": "commandTemplate",
      "value": "oscar-cli service put-file fish-detector {source}"
    },
    {
      "@id": "#action-async-get",
      "@type": "TransferAction",
      "name": "service get-file",
      "target": {
        "@id": "#entry-async-get"
      },
      "result": {
        "@id": "#expected-image"
      },
      "additionalProperty": [
        {
          "@id": "#command-template-async-get"
        }
      ]
    },
    {
      "@id": "#entry-async-get",
      "@type": "EntryPoint",
      "actionApplication": {
        "@id": "#oscar-cli"
      }
    },
    {
      "@id": "#command-template-async-get",
      "@type": "PropertyValue",
      "propertyID": "commandTemplate",
      "value": "oscar-cli service get-file fish-detector --download-latest-into {destination}/input.jpg/"
    },
